{
    "name": "vue-chill-code-input",
    "version": "0.1.0",
    "description": "Easy Verification Code Input for your Vue projects",
    "author": "Tibor Skvrnda <tibor@skvrnda.sk> (https://skvrnda.sk/)",
    "keywords": [
        "vue",
        "input",
        "code",
        "verification",
        "typescript",
        "pincode",
        "pin"
    ],
    "homepage": "https://github.com/tskvrnda/vue-chill-code-input#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/tskvrnda/vue-chill-code-input"
    },
    "bugs": {
        "url": "https://github.com/tskvrnda/vue-chill-code-input/issues"
    },
    "license": "MIT",
    "main": "dist/vue-chill-code-input.ssr.js",
    "browser": "dist/vue-chill-code-input.esm.js",
    "module": "dist/vue-chill-code-input.esm.js",
    "unpkg": "dist/vue-chill-code-input.min.js",
    "types": "vue-chill-code-input.d.ts",
    "files": [
        "dist/*",
        "vue-chill-code-input.d.ts",
        "src/**/*.vue"
    ],
    "sideEffects": false,
    "scripts": {
        "serve": "vue-cli-service serve dev/serve.ts",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "prepublish": "npm run build"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@vue/cli-plugin-babel": "^4.5.10",
        "@vue/cli-plugin-typescript": "^4.5.10",
        "@vue/cli-service": "^4.5.10",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.5",
        "rollup": "^2.36.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.9",
        "typescript": "^3.8.3",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    },
    "peerDependencies": {
        "vue": "^2.6.12"
    },
    "engines": {
        "node": ">=12"
    }
}
